[color]
        ui = auto
[alias]
        co = checkout
        ci = commit
        st = status
        br = branch
        rb = rebase
        undo = reset --hard
        hist = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short
        graph = log --oneline --graph
        type = cat-file -t
        dump = cat-file -p
